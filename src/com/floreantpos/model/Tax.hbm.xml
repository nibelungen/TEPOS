<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.floreantpos.model">
	<class name="com.floreantpos.model.Tax" table="TAX" lazy="false" optimistic-lock="version">
		
		<id name="id" type="java.lang.Integer" column="ID">
			<generator class="identity" />
		</id>
		
		<version name="modifiedTime" column="MODIFIED_TIME" type="timestamp"></version>

		<property name="name" type="java.lang.String" column="NAME" not-null="true" length="20" />
		<property name="rate" type="java.lang.Double" column="RATE" length="52" />

		<!-- Associations -->

	</class>
</hibernate-mapping>
