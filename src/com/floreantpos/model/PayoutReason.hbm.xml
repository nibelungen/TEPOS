<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
    <hibernate-mapping package="com.floreantpos.model" default-lazy="false">
    	<class name="PayoutReason" table="PAYOUT_REASONS" lazy="false">
    		<id name="id" column="ID" type="java.lang.Integer">
    			<generator class="identity"></generator>
    		</id>
    		
    		<property name="reason" column="REASON" type="string" length="255"></property>
    	</class>
    </hibernate-mapping>