<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.mdss.pos.model">
	<class name="com.mdss.pos.model.RestaurantTable" table="RESTAURANT_TABLE" lazy="false">
		
		<id name="id" type="java.lang.Integer" column="ID">
			<generator class="identity" />
		</id>

		<property name="capacity" type="java.lang.Integer" column="CAPACITY" length="10" />
		<property name="name" type="java.lang.String" column="NAME" length="60" />
		<property name="booked" type="java.lang.Boolean" column="BOOKED" />

		<!-- Associations -->


	</class>
</hibernate-mapping>
