<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
    
    <hibernate-mapping package="com.mdss.pos.model">
        <class name="CashDrawerResetHistory" table="CASH_DRAWER_RESET_HISTORY" lazy="false">
            <id name="id" column="ID" type="java.lang.Integer">
                <generator class="identity"></generator>
            </id>
            
            <property name="resetTime" column="RESET_TIME" type="java.util.Date" index="drawer_reset_time"></property>
            
            <one-to-one name="drawerPullReport" class="DrawerPullReport" lazy="false">
            	
            </one-to-one>
            
            <many-to-one name="resetedBy" class="com.mdss.pos.model.User" lazy="false">
            	<column name="USER_ID"></column>
            </many-to-one>
            
        </class>
   </hibernate-mapping>